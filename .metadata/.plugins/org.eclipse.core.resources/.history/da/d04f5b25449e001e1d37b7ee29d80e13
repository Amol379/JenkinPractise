public class Test {
	public static void main(String[] args) {

		int[] arr = { 1, 2, 3, 5, 8, 3 };
		/*
		 * List<int[]> collect =
		 * Arrays.asList(arr).stream().sorted(Comparator.comparingInt(a -> (int)
		 * a).reversed()) .collect(Collectors.toList());
		 * collect.forEach(System.out::println);
		 */
		int temp = 0;

		for (int i = arr.length-1; i > 0; i--) {
			System.out.println(i);
			temp = arr[i];
			arr[i]= arr[i+1];
			arr[i]= temp;

		}
		System.out.println(arr[1]);
	}

}
