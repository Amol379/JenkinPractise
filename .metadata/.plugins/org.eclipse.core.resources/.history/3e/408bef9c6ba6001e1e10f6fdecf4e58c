package OOPS;

class Parent {

	public void dispaly() {
		System.out.println("i am parent");
	}
}

class Child extends Parent {
	protected void display() {
		System.out.println("i am child");
	}
}

public class TestInheritance {
	public static void main(String[] args) {
		Parent parent = new Child();
		parent.dispaly();
	}
}
